  <select id="getDeskDepByDeskId" resultMap="BaseResultMap2">
    select thdd.*,thwug.NAME as GROUP_NAME
    from t_ha_desk_dep thdd
    left join t_ha_work_user_group thwug
        on thdd.GROUP_ID = thwug.ID
    where thdd.DELETE_STATUS = '1'
      and  thdd.DESK_ID  = #{deskId} ;
  </select>

  <select id="getDeskDepByGroupId" resultMap="BaseResultMap">
    select * from t_ha_desk_dep where DELETE_STATUS = '1' and  GROUP_ID  = #{groupId} and CONFIRM_FLAG = '0' ;
  </select>

  <select id="getListWithPage" resultMap="BaseResultMap2">
    select thdd.*,
           thd.*,
           thwq.*
    from t_ha_desk_dep thdd
    left join t_ha_desk thd on thd.ID = thdd.DESK_ID
    left join t_ha_work_queue thwq on thd.QUEUE_ID = thwq.ID
    <where>
      thd.DELETE_STATUS = '1'
      <if test="name != null and name != '' ">
        and thwq.NAME like concat('%', #{name}, '%')
      </if>
      <if test="phone != null and phone != '' ">
        and thwq.PHONE = #{phone}
      </if>
      <if test="companyCode != null and companyCode != '' ">
        and thd.COMPANY_CODE = #{companyCode}
      </if>
      <if test="companyName != null and companyName != '' ">
        and thd.COMPANY_NAME like concat('%', #{companyName}, '%')
      </if>
      <if test="beginTime != null"><!-- 开始时间检索 -->
        AND thdd.CON_DATE &gt;= #{beginTime}
      </if>
      <if test="endTime != null "><!-- 结束时间检索 -->
        AND thd.CON_DATE &lt;= #{endTime}
      </if>
      <if test="deskStatus != null and deskStatus != ''">
        AND thd.DESK_STATUS = #{deskStatus}
      </if>
      <if test="workUserId != null and workUserId != ''">
        AND thdd.CON_WORK_USER_ID = #{workUserId}
      </if>
    </where>
  </select>

