  <select id="getListWithPage" resultMap="BaseResultMap2">
    select thd.*,
           thwq.*
    from t_ha_desk thd
    left join t_ha_work_queue thwq on thd.QUEUE_ID = thwq.ID
    <where>
        thd.DELETE_STATUS = '1'
      <if test="name != null and name != '' ">
        and thwq.NAME like concat('%', #{name}, '%')
      </if>
      <if test="phone != null and phone != '' ">
        and thwq.PHONE = #{phone}
      </if>
      <if test="companyCode != null and companyCode != '' ">
        and thd.COMPANY_CODE = #{companyCode}
      </if>
      <if test="companyName != null and companyName != '' ">
        and thd.COMPANY_NAME like concat('%', #{companyName}, '%')
      </if>
      <if test="beginTime != null"><!-- 开始时间检索 -->
        AND thd.CREATE_DATE &gt;= #{beginTime}
      </if>
      <if test="endTime != null "><!-- 结束时间检索 -->
        AND thd.CREATE_DATE &lt;= #{endTime}
      </if>
      <if test="deskStatus != null and deskStatus != ''">
        AND thd.DESK_STATUS = #{deskStatus}
      </if>
      <if test="workUserId != null and workUserId != ''">
        AND thd.WORK_USER_ID = #{workUserId}
      </if>
    </where>
    order by thd.CREATE_DATE desc
  </select>

